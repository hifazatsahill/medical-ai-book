{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/src/main.py"
    }
  ],
  "functions": {
    "backend/src/main.py": {
      "maxDuration": 60
    }
  }
}